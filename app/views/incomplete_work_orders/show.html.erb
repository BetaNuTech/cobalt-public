

<% content_for :header do %>
  <div id="application_header_logo">
      <%= link_to root_path, data: { turbolinks: false } do %>
          <%= show_svg('cobalt_report_white.svg') %>
      <% end %>
  </div>
  <span id="header_title">Incomplete Work Orders for <strong><%=@property_name%></strong> for <strong><%=@date%></strong></span>
<% end %>

<div id="incomplete_work_orders">  
  <table id="incomplete_work_orders_table" class="stripe" width="100%" cellpadding="0" cellspacing="0" border="0" class="table" >
    <thead>
      <th>
        Call Date
      </th>
      <th>
        Update Date
      </th>
      <th>
        Work Order
      </th>
      <th>
        Unit
      </th>
      <th>
        Brief Description
      </th>
      <th>
        Reason Incomplete
      </th>
    </thead>
    <tbody>
    </tbody>
  </table>
</div>
