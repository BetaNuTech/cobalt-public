<link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">

<% content_for :header do %>
    <div id="application_header_logo">
        <%= link_to root_path, data: { turbolinks: false } do %>
            <%= show_svg('cobalt_recruiting_white.svg') %>
        <% end %>
    </div>
<% end %>

<div id="workable_jobs_index_container">

    <div id="workable_jobs_export"><%= button_to "Export Jobs", workable_jobs_export_path(format: "csv"), method: :post %></div>

    <div id="workable_jobs_alert_counts">
        <span class="workable_jobs_alert_count workable_jobs_bkg_color_red workable_jobs_color_white"><%= @alerts_lvl2_count %></span>
        <span class="workable_jobs_alert_count workable_jobs_bkg_color_darkorange workable_jobs_color_white"><%= @alerts_lvl1_count %></span>
        <%= form_tag workable_jobs_path, method: 'get' do %>
            <%= select_tag :sort_by, options_for_select(@sort_options, @sort_option_selected) %>
            <%= submit_tag "Update Sort", class: "btn btn-primary" %>
        <% end %>
    </div>


