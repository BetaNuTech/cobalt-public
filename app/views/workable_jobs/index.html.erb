        <div class="workable_jobs_flex_container_vertical">
            <svg class="workable_jobs_svg_job_search"><%= show_svg('job-search-in-newspapers.svg') %></svg>
            <p class="workable_jobs_text_section_title workable_jobs_color_white">360</p>
        </div>

        <%# 360 JOBS TABLE %>
        <div class="workable_jobs_jobs_table">
            <%# TABLE HEADER %>
            <div class="workable_jobs_jobs_table_header workable_jobs_bkg_color_white workable_jobs_grid_container_jobs_table_header">
                <p class="workable_jobs_color_blue workable_jobs_text_jobs_table_header" >Opened</p>
                <p class="workable_jobs_color_blue workable_jobs_text_jobs_table_header" >Days</p>
                <p class="workable_jobs_color_blue workable_jobs_text_jobs_table_header" >Region</p>
                <p class="workable_jobs_color_blue workable_jobs_text_jobs_table_header" >Activity</p>
                <p class="workable_jobs_color_blue workable_jobs_text_jobs_table_header" >Activity</p>
                <p class="workable_jobs_color_blue workable_jobs_text_jobs_table_header" >Notes / Alerts</p>
            </div>
            <% @open_360_jobs_data.each_with_index do |element, index| %>
                <div class="workable_jobs_jobs_table_row workable_jobs_bkg_color_jobs_table_row_<%= index.odd? ? 'odd' : 'even' %>">
                    <p class="workable_jobs_color_white workable_jobs_jobs_table_row_title workable_jobs_text_jobs_table_row_title">
                        <% if element[:state] == 'closed' %>
                            UNPUBLISHED: 
                        <% end %>
                        <% if element[:url].present? %>
                            <a target="_blank" href="<%= element[:url] %>"><%= element[:title] %></a>
                        <% else %>
                            <%= element[:title] %>
                        <% end %>
                    </p>
                </div>
                <div class="workable_jobs_jobs_table_row workable_jobs_grid_container_jobs_table_row workable_jobs_bkg_color_jobs_table_row_<%= index.odd? ? 'odd' : 'even' %>">
                    <p class="workable_jobs_color_white workable_jobs_text_jobs_table_row">
                        <%= element[:date_opened] %>
                    </p>
                    <p class="workable_jobs_color_white workable_jobs_text_jobs_table_row">
                        <% if element[:time_to_start].nil? %>
                            <%= element[:days_open] %>
                        <% end %>
                        <% if element[:time_to_fill].present? %>
                            <% if element[:time_to_start].nil? %><br><% end %>
                            <span class="workable_jobs_color_white workable_jobs_text_jobs_table_row_time_to_title"><%= element[:time_to_fill] %></span>
                            <br>
                            <span class="workable_jobs_color_off_white workable_jobs_text_jobs_table_row_time_to_sub_text">days2fill</span>
                        <% end %>
                        <% if element[:time_to_start].present? %>
                            <br>
                            <span class="workable_jobs_color_white workable_jobs_text_jobs_table_row_time_to_title"><%= element[:time_to_start] %></span>
                            <br>
                            <span class="workable_jobs_color_off_white workable_jobs_text_jobs_table_row_time_to_sub_text">days2start</span>
                        <% end %>
                    </p>
                    <p class="workable_jobs_color_white workable_jobs_text_jobs_table_row">
                        <% if element[:team_logo].present? %>
                            <%= retina_image_tag(element[:team_logo], :small, :class => "workable_jobs_team_logo_white_bkgrd") %>
                            <br>
                        <% end %>
                        <%= element[:code] %>
                    </p>
                    <p class="workable_jobs_color_white workable_jobs_text_jobs_table_row">
                        <% if element[:last_activity_member_profile_image].present? %>
                            <%= retina_image_tag(element[:last_activity_member_profile_image], :small, :class => "workable_jobs_member_img") %>
                            <br>
                        <% end %>
                        <% if element[:last_activity_member_slack_dm_url].nil? %>
                            <%= element[:last_activity_member_name] %><br>
                        <% else %>
                            <a href="<%= element[:last_activity_member_slack_dm_url] %>"><%= element[:last_activity_member_name] %></a><br>
                        <% end %>
                        <span class="workable_jobs_color_off_white workable_jobs_text_jobs_table_row_sub_text">
                            <%= element[:last_activity_member_action] %><br>
                            <%= element[:last_activity_member_date] %>
                        </span>
                    </p>
                    <p class="workable_jobs_color_off_white workable_jobs_text_jobs_table_row_sub_text"><%= element[:last_activity_candidate_action] %><br><%= element[:last_activity_candidate_date] %></p>
                    <p class="workable_jobs_text_jobs_table_row_sub_text">
                        <% element[:notes_alerts].each do |note_alert| %>
                            <span class="workable_jobs_bkg_color_<%= note_alert[:bkgrd_color]%> workable_jobs_color_<%= note_alert[:color]%>"><%= note_alert[:text] %></span><br>
                        <% end %>
                    </p>
                </div>
            <% end %>
        </div>
    </div>

